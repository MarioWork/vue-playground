{
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:vue/vue3-recommended",
    "plugin:json/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["json", "vue"],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "error",
    "json/*": ["error", "allowComments"],
    "vue/html-self-closing": [
      "warn",
      {
        "html": {
          "void": "any"
        }
      }
    ]
  },
  "settings": {}
}
